#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
void BubbleSort(int* p, int n)
{
	int end = n;
	while (end >0)
	{
		int flag = 0;
		for (int i = 1; i < end; i++)
		{
			if (p[i - 1] > p[i])
			{
				int tem = p[i];
				p[i] = p[i - 1];
				p[i - 1] = tem;
				flag = 1;
			}
		}
		if (flag == 0)
		{
			break;
		}
		--end;
	}
}
int main()
{
	int arr[7], sum = 0;
	float avg;
	for (int i = 0; i < 7; i++)
	{
		scanf("%d", &arr[i]);
	}
	BubbleSort(arr, 7);
	for (int i = 1; i < 6; i++)
	{
		sum += arr[i];
	}
	avg = (float)sum / 5;
	printf("%.2f", avg);
	return 0;
}
