#include<stdio.h>
int main()
{
    int arr[10];
    int positive = 0;
    int negative = 0;
    for(int i = 0;i < 10;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i = 0;i < 10;i++)
    {
        if(arr[i] > 0)
        {
            positive += 1;
        }
        else if(arr[i] < 0)
        {
            negative += 1;
        }
    }
    printf("positive:%d\n",positive);
    printf("negative:%d\n",negative);
    return 0;
}
