struct ListNode *detectCycle(struct ListNode *head) 
{
        struct ListNode * fast = head;
        struct ListNode * slow = head;
        //先判断是否带环
        while(fast && fast->next)
        {
            slow = slow->next;
            fast = fast->next->next;
            if(slow == fast)
            {
                //相遇的节点给meet
                struct ListNode * meet = slow; 
                //带环，给两个指针一个head一个meet，两个指针同时走，直到相遇，相遇点就是入环的节点
                 while(meet != head)
                 {
                     head = head->next;
                     meet = meet->next;
                }
                return head;
            }
        }
        return NULL;
}
